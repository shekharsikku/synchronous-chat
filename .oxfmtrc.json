{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "jsxSingleQuote": false,
  "printWidth": 120,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "experimentalSortPackageJson": false,
  "ignorePatterns": ["node_modules", ".idea", ".vscode", ".env", ".env.*", "**/dist", "**/test"]
}
